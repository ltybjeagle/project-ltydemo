  <modelVersion>4.0.0</modelVersion>

  <artifactId>platform-basecommon-boot</artifactId>

  <name>platform-basecommon-boot</name>
  <packaging>jar</packaging>
  <url>http://www.example.com</url>

  <dependencies>
    <dependency>
      <groupId>org.springframework.boot</groupId>
      <artifactId>spring-boot-starter-web</artifactId>
    </dependency>
    <dependency>
      <groupId>org.springframework.boot</groupId>
      <artifactId>spring-boot-starter-actuator</artifactId>
    </dependency>
    <dependency>
      <groupId>com.sunny.maven</groupId>
      <artifactId>platform-basecommon-cache</artifactId>
      <version>1.0-SNAPSHOT</version>
    </dependency>
  </dependencies>

  <build>
    <plugins>
      <plugin>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-maven-plugin</artifactId>
        <configuration>
          <executable>true</executable>
          <mainClass>com.sunny.maven.AppBootApplication</mainClass>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
